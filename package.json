{
  "name": "bhikkhupatimokkha",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write . && DEBUG=eslint:eslint eslint ./src --fix",
    "lint": "prettier --check . && DEBUG=eslint:eslint eslint ./src",
    "deploy": "vite build && NODE_DEBUG=gh-pages gh-pages --dist ./build --no-history --nojekyll"
  },
  "devDependencies": {
    "@danielx/civet": "^0.8.14",
    "@eslint/js": "^9.14.0",
    "@fontsource/ibm-plex-sans-thai-looped": "^5.1.0",
    "@fontsource/maitree": "^5.1.0",
    "@fontsource/noto-serif-thai": "^5.1.0",
    "@fontsource/thasadith": "^5.1.0",
    "@rollup/plugin-dsv": "^3.0.5",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bun": "^1.1.13",
    "autoprefixer": "^10.4.20",
